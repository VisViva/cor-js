{
    "name": "scene-manager",
    "version": "1.0.0",
    "description": "Scene manager",
    "scripts": {
        "win:start": "webpack-dev-server --config \"webpack/webpack.server.config.js\" --hot --inline",
        "win:test:watch:phantomjs": "rimraf reports && karma start \"karma/karma.watch.phantomjs.config.js\"",
        "win:test:watch:all": "rimraf reports && karma start \"karma/karma.watch.all.config.js\"",
        "win:test:once": "rimraf reports && karma start \"karma/karma.once.config.js\"",
        "win:build": "rimraf dist && npm run test:once && webpack --config \"webpack/webpack.build.config.js\"",
        "start": "webpack-dev-server --config './webpack/webpack.server.config.js' --hot --inline",
        "test:watch:phantomjs": "rimraf reports && karma start './karma/karma.watch.phantomjs.config.js'",
        "test:watch:all": "rimraf reports && karma start './karma/karma.watch.all.config.js'",
        "test:once": "rimraf reports && karma start './karma/karma.once.config.js'",
        "build": "rimraf dist && webpack --config './webpack/webpack.build.config.js'"
    },
    "devDependencies": {
        "babel-core": "^6.14.0",
        "babel-loader": "^6.2.5",
        "babel-preset-es2015": "^6.14.0",
        "chai": "^3.5.0",
        "del": "^2.2.2",
        "eslint": "^3.6.1",
        "eslint-loader": "^1.5.0",
        "gl-matrix": "^2.3.2",
        "gulp": "^3.9.1",
        "gulp-rename": "^1.2.2",
        "gulp-uglify": "^2.0.0",
        "html-webpack-plugin": "^2.22.0",
        "istanbul": "^0.4.5",
        "istanbul-instrumenter-loader": "^1.0.0",
        "jasmine": "^2.5.1",
        "jasmine-core": "^2.5.1",
        "karma": "^1.3.0",
        "karma-chai": "^0.1.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-coverage": "^1.1.1",
        "karma-firefox-launcher": "^1.0.0",
        "karma-html-reporter": "^0.2.7",
        "karma-jasmine": "^1.0.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "karma-mocha": "^1.2.0",
        "karma-mocha-reporter": "^2.2.0",
        "karma-phantomjs-launcher": "^1.0.2",
        "karma-sinon": "^1.0.5",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-typescript-preprocessor2": "^1.2.1",
        "karma-webpack": "^1.8.0",
        "mocha": "^3.1.0",
        "phantomjs": "^2.1.7",
        "phantomjs-prebuilt": "^2.1.12",
        "rimraf": "^2.5.4",
        "sinon": "^1.17.4",
        "webpack": "^1.13.2",
        "webpack-dev-server": "~1.16.1",
        "webpack-shell-plugin": "^0.4.3"
    }
}
